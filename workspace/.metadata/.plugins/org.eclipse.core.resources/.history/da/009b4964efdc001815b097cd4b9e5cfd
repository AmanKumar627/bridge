package com.bridge;

public class Prime {
	public static void checkPrime(int number){
		int flag=0;
		for(int i=2;i<number;i++)
		{
			if(number%i==0)
			{
				flag++;
				break;
			}
		}
		if(flag==0)
		{
			System.out.println(number+" is a prime number");
		}
		else
		{
			System.out.println(number+" is not a prime number");
		}
		
	}

}
